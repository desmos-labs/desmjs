{
  "name": "desmjs-documentation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "NODE_OPTIONS=--max_old_space_size=6144 docusaurus start",
    "build": "NODE_OPTIONS=--max_old_space_size=6144 docusaurus build && echo $DOCS_DOMAIN > build/CNAME",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "write-cname": "",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "^2.4.3",
    "@docusaurus/preset-classic": "^2.4.3",
    "@swc/core": "^1.3.100",
    "@you54f/theme-github-codeblock": "^0.1.1",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swc-loader": "^0.2.3"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.0.0",
    "@tsconfig/docusaurus": "^2.0.2",
    "docusaurus-plugin-typedoc": "^0.21.0",
    "typedoc": "^0.25.4",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.1.6"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  },
  "overrides": {
    "@mdx-js/react": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
